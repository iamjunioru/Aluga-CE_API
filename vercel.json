{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/uploads/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "(.*)",
            "dest": "src/index.ts"
        }
    ]
}
