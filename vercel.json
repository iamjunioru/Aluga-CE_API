{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
        "src": "/(.*)",
        "dest": "src/public/" 
    },
    {
      "src": "(.*)",
      "dest": "src/index.ts"
    }
  ]
}
