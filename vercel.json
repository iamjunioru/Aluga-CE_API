{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "src/public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "src/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "src/public/$1"
        }
    ]
}