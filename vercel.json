{
    "version": 2,
    "public": true,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "src/public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "(.*)",
            "dest": "src/index.ts"
        }
    ]
}
